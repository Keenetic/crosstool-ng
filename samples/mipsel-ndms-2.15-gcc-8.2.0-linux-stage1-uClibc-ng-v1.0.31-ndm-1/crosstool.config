CT_EXPERIMENTAL=y
# CT_PREFIX_DIR_RO is not set
CT_CREATE_TARBALL=y
CT_PARALLEL_JOBS=16
CT_EXTRA_CFLAGS_FOR_HOST="-O2"
# CT_LOG_TO_FILE is not set
CT_ARCH_MIPS=y
CT_ARCH_TUNE="mips32r2"
CT_ARCH_SUFFIX="el"
CT_ARCH_LE=y
CT_ARCH_FLOAT_SW=y
CT_TARGET_CFLAGS="-pipe -O2 -mips32r2 -g1 -fno-omit-frame-pointer"
CT_TOOLCHAIN_PKGVERSION="NDMS Toolkit 1.0.0"
CT_TOOLCHAIN_BUGURL="https://forum.keenetic.net/"
CT_TARGET_VENDOR="ndms"
CT_TARGET_VENDOR_SAMPLE_REV="2.15"
CT_KERNEL_LINUX=y
CT_LINUX_SRC_DEVEL=y
CT_LINUX_DEVEL_URL="https://github.com/ndmsystems/kernel-49.git"
CT_LINUX_DEVEL_BRANCH="refs/heads/stage1"
CT_LINUX_V_4_9=y
CT_LIBC_UCLIBC=y
CT_UCLIBC_NG_SRC_DEVEL=y
CT_UCLIBC_NG_DEVEL_URL="https://github.com/ndmsystems/uClibc-ng.git"
CT_UCLIBC_NG_DEVEL_REVISION="v1.0.31-ndm-1"
CT_LIBC_UCLIBC_CONFIG_FILE="${CT_LIB_DIR}/samples/mipsel-ndms-2.15-gcc-8.2.0-linux-stage1-uClibc-ng-v1.0.31-ndm-1/uClibc-ng-v1.0.31-ndm-1.config"
CT_LIBC_UCLIBC_LOCALES=y
CT_LIBC_UCLIBC_IPV6=y
CT_CC_GCC_CONFIG_TLS=y
CT_CC_GCC_LIBSSP=y
CT_CC_GCC_LIBSANITIZER=y
CT_CC_LANG_CXX=y
CT_DEBUG_GDB=y
CT_GDB_V_7_11_1=y
CT_GDB_NATIVE=y
CT_GDB_NATIVE_EXTRA_CONFIG_ARRAY="--disable-gdbmi --with-expat=no --enable-tui=no --with-curses=no"
CT_GDB_NATIVE_EXTRA_CFLAGS="-Os -ffunction-sections -fdata-sections"
CT_GDB_NATIVE_EXTRA_CXXFLAGS="-Os -ffunction-sections -fdata-sections"
CT_GDB_NATIVE_EXTRA_LDFLAGS="-Wl,--gc-sections"
CT_DEBUG_LTRACE=y
CT_DEBUG_STRACE=y
CT_GCC_V_8_2_0=y
