CT_LOCAL_TARBALLS_DIR="${HOME}/src"
CT_SAVE_TARBALLS=y
# CT_INSTALL_DIR_RO is not set
CT_CREATE_TARBALL=y
CT_LOG_EXTRA=y
CT_ARCH_ARCH="mips32r2"
CT_ARCH_LE=y
CT_ARCH_FLOAT_SW=y
CT_ARCH_mips=y
CT_TOOLCHAIN_PKGVERSION="NDMS Toolkit 1.0.0"
CT_TOOLCHAIN_BUGURL="http://keenopt.ru"
CT_TARGET_VENDOR="ndms"
CT_KERNEL_linux=y
CT_KERNEL_GIT=y
CT_KERNEL_GIT_URL="git@github.com:ndmsystems/linux-3.4.git"
CT_KERNEL_GIT_CSET="ref=3.4-53"
CT_BINUTILS_V_2_24=y
CT_BINUTILS_PLUGINS=y
CT_LIBC_uClibc=y
CT_LIBC_UCLIBC_GIT=y
CT_LIBC_UCLIBC_GIT_URL="git@github.com:ndmsystems/uClibc-ng.git"
CT_LIBC_UCLIBC_GIT_CSET="ref=v1.0.18-ndm-1"
CT_LIBC_UCLIBC_CONFIG_FILE="${CT_LIB_DIR}/samples/mipsel-ndms-gcc-5.4.0-linux-3.4-uclibc/uClibc-v1.0.18.config"
CT_LIBC_UCLIBC_WCHAR=y
CT_CC_GCC_V_5_4_0=y
CT_CC_LANG_CXX=y
CT_DEBUG_gdb=y
# CT_GDB_CROSS_PYTHON is not set
CT_GDB_NATIVE=y
# CT_GDB_GDBSERVER_STATIC is not set
CT_GDB_V_7_4_1=y
